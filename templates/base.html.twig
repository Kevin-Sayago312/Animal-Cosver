<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <!-- favicon -->
        <link rel="shortcut icon" type="image/png" href="../assets/img/favicon.png">
            <!-- google font -->
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Poppins:400,700&display=swap" rel="stylesheet">
            <!-- fontawesome -->
        <link rel="stylesheet" href="../assets/css/all.min.css">
        <!-- bootstrap -->
        <link rel="stylesheet" href="../assets/bootstrap/css/bootstrap.min.css">
        <!-- owl carousel -->
        <link rel="stylesheet" href="../assets/css/owl.carousel.css">
        <!-- magnific popup -->
        <link rel="stylesheet" href="../assets/css/magnific-popup.css">
        <!-- animate css -->
        <link rel="stylesheet" href="../assets/css/animate.css">
        <!-- mean menu css -->
        <link rel="stylesheet" href="../assets/css/meanmenu.min.css">
        <!-- main style -->
        <link rel="stylesheet" href="../assets/css/main.css">
        <!-- responsive -->
        <link rel="stylesheet" href="../assets/css/responsive.css">
        <!-- iconos de bootstrap -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
        <!-- CSS DE BOLA FLOTANTE -->
        <link rel="stylesheet" href="../assets/css/bola_flotante.css">
        <!-- CSS DE APARTADO PARA MI PERFIL -->
        <link rel="stylesheet" href="styles2.css">
        {% endblock %}

       
    </head>
    <body>
        {% block body %}
        <!--EMPIEZA LA PRE RECARGA DE LA PAGINA-->
    <div class="loader">
		<div class="loader-inner">
			<div class="zigzag-line"></div>
		</div>
	</div>
    <!--TERMINA LA PRE RECARGA DE LA PAGINA-->
	
	<!-- EMPIEZA HEADER -->
	<div class="top-header-area" id="sticker">
		<div class="container">
			<div class="row">
				<div class="col-lg-12 col-sm-12 text-center">
					<div class="main-menu-wrap">

						<!-- menu start -->
						<nav class="main-menu">
							<ul>
								<li class="current-list-item"><a href="{{path('app_index')}}">Inicio</a>
								</li>
								<li><a href="{{path('app_test')}}">Realizar examen</a>
								<!--<ul class="sub-menu">
									<li><a href="contact.html">Contáctanos</a></li>
								</ul>-->
							    </li>
								
								<li>
									<div class="header-icons">
										<li>
											{% if app.user %}
											<a href="{{path('app_login')}}" class="my-auto">
												Mi Perfil
											</a>
											<ul class="sub-menu">
												{% if app.user.photo %}
                                                <img src="{{ asset('uploads/photos/' ~ app.user.photo) }}" alt="Foto de perfil" style="max-width: 50px; max-height: 50px;" />
                                                {% else %}
                                                {# Si el usuario no tiene foto, mostrar una imagen predeterminada #}
                                                <img src="{{ asset('public/assets/img/defecto.png') }}" alt="Foto de perfil predeterminada" style="max-width: 50px; max-height: 50px;" />
                                                {% endif %}
												<li><a href="{{path('app_mi_perfil')}}">Mi perfil</a></li>
												<li><a href="{{ path('app_logout') }}">Cerrar Sesion</a></li>
											</ul>
										{% else %}
										<li>
											<a href="{{path('app_login')}}" class="my-auto">
												Iniciar Sesion
											</a>
										{% endif %}
										</li>
										<!-- CODIGO PARA PONER UN ICONO DE BUSQUEDA -->
										<!--<a class="mobile-hide search-bar-icon" href="#"><i class="fas fa-search"></i></a> -->
									</div>
								</li>
							</ul>
						</nav>
						<a class="mobile-show search-bar-icon" href="#"><i class="fas fa-search"></i></a>
						<div class="mobile-menu"></div>
						<!-- menu end -->
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- TERMINA HEADER -->
	<nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="#">Mi CRUD</a>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('producto_index') }}">Productos</a>
                    </li>
                </ul>
                {% if app.user %}
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('app_logout') }}">Cerrar sesión</a>
                        </li>
                    </ul>
                {% else %}
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('app_login') }}">Iniciar sesión</a>
                        </li>
                    </ul>
                {% endif %}
            </div>
        </nav>
        {% endblock %}

		{% block footer %}
		<!-- EMPIEZA FOOTER -->
	<div class="footer-area">
		<div class="container">
			<div class="row">
				<div class="col-lg-3 col-md-6">
					<div class="footer-box about-widget">
						<h2 class="widget-title">Acerca de la página</h2>
						<p>Esta pagina es una herramienta para ayudar a los amantes de los perros
						</p>
					</div>
				</div>
				<div class="col-lg-3 col-md-6">
					<div class="footer-box get-in-touch">
						<h2 class="widget-title">Encuentranos en</h2>
						<ul>
							<li>Cosamaloapan, Veracruz</li>
							<li>isc.kevin.sayago.rosales@gmail.com</li>
							<li>(+52) 288-141-4421</li>
						</ul>
					</div>
				</div>
				<div class="col-lg-3 col-md-6">
					<div class="footer-box pages">
						<h2 class="widget-title">Apartados</h2>
						<ul>
							<li><a href="index.html">Inicio</a></li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- TERMINA FOOTER -->
	
	<!-- EMPIEZA copyright -->
	<div class="copyright">
		<div class="container">
			<div class="row">
				<div class="col-lg-6 col-md-12">
					<p>Copyrights &copy; 2024 - <a href="#"> ANIMAL COSVER</a>,  Todos los derechos reservados.<br>
					</p>
				</div>
				<div class="col-lg-6 text-right col-md-12">
					<div class="social-icons">
						<ul>
							<li><a href="#" target="_blank"><i class="fab fa-facebook-f"></i></a></li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- TERMINA copyright -->
		{% endblock %}

		<!-- EMPIEZA DIRECCIONES DE LOS SCRIPTS -->
        {% block javascripts %}
       <!-- jquery -->
	<script src="assets/js/jquery-1.11.3.min.js"></script>
	<!-- bootstrap -->
	<script src="assets/bootstrap/js/bootstrap.min.js"></script>
	<!-- count down -->
	<script src="assets/js/jquery.countdown.js"></script>
	<!-- isotope -->
	<script src="assets/js/jquery.isotope-3.0.6.min.js"></script>
	<!-- waypoints -->
	<script src="assets/js/waypoints.js"></script>
	<!-- owl carousel -->
	<script src="assets/js/owl.carousel.min.js"></script>
	<!-- magnific popup -->
	<script src="assets/js/jquery.magnific-popup.min.js"></script>
	<!-- mean menu -->
	<script src="assets/js/jquery.meanmenu.min.js"></script>
	<!-- sticker js -->
	<script src="assets/js/sticker.js"></script>
	<!-- main js -->
	<script src="assets/js/main.js"></script>


	
     {% endblock %}
        <!-- TERMINAN DIRECCIONES DE LOS SCRIPTS -->
    </body>
</html>
